<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.lemzy.librarymanagementsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MyWebViewActivity"></activity>
        <activity android:name=".CurrentlyReadingActivity" />
        <activity
            android:name=".AlreadyReadActivity"
            android:label="Already Read Books" />
        <activity
            android:name=".WantToActivity"
            android:label="Want To Read Books" />
        <activity
            android:name=".BookActivity"
            android:label="Books" />
        <activity
            android:name=".ShowAllActivity"
            android:label="All Books" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>